<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <configSections>
        <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
    </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <appSettings>
        <add key="TelegramClientPhoneNumber" value="" />
        <add key="TelegramApiId" value="" />
        <add key="TelegramApiHash" value="" />
        <add key="TelegramPhoneCodeHash" value="" />
        <add key="TelegramMTProtoServerHost" value="" />
        <add key="TelegramMTProtoServerPort" value="" />
    </appSettings>
    <log4net>
        <appender name="ConsoleAppender" type="log4net.Appender.ConsoleAppender" >
            <layout type="log4net.Layout.PatternLayout">
                <conversionPattern value="[%-5p][%t] %d %c - %m%n" />
            </layout>
        </appender>
        <root>
            <level value="INFO" />
            <appender-ref ref="ConsoleAppender" />
        </root>
    </log4net>
</configuration>
